<?php

namespace AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Gedmo\Mapping\Annotation as Gedmo;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * CmsStaticPageImage
 * @ORM\Table(name="cms_static_page_image")
 * @ORM\Entity(repositoryClass="AdminBundle\Repository\CmsStaticPageImageRepository")
 */
class CmsStaticPageImage
{
    /**
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    
    /**
     * @ORM\Column(type="string", nullable=false)
     * @Assert\NotBlank()
     */
    private $fileName;

    /**
     * @ORM\Column(type="integer", nullable=true)
     */
    private $sort;


    /**
     * @ORM\Column(type="string", length=3, nullable=true)
     */
    private $extension;
    
    /**
     * @ORM\Column(type="datetime", nullable=false)
     * @Gedmo\Timestampable(on="create")
     */
    private $createdAt;

    /**
     * @ORM\ManyToOne(targetEntity="AdminBundle\Entity\CmsStaticPage", inversedBy="images")
     */
    private $cmsStaticPage;
    
    /**
     * Get id
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set fileName
     *
     * @param string $fileName
     *
     * @return CmsStaticPageImage
     */
    public function setFileName($fileName)
    {
        $this->fileName = $fileName;

        return $this;
    }

    /**
     * Get fileName
     *
     * @return string
     */
    public function getFileName()
    {
        return $this->fileName;
    }

    /**
     * Set sort
     *
     * @param integer $sort
     *
     * @return CmsStaticPageImage
     */
    public function setSort($sort)
    {
        $this->sort = $sort;

        return $this;
    }

    /**
     * Get sort
     *
     * @return integer
     */
    public function getSort()
    {
        return $this->sort;
    }

    /**
     * Set cmsStaticPage
     *
     * @param \AdminBundle\Entity\CmsStaticPage $cmsStaticPage
     *
     * @return CmsStaticPageImage
     */
    public function setCmsStaticPage(\AdminBundle\Entity\CmsStaticPage $cmsStaticPage = null)
    {
        $this->cmsStaticPage = $cmsStaticPage;

        return $this;
    }

    /**
     * Get cmsStaticPage
     *
     * @return \AdminBundle\Entity\CmsStaticPage
     */
    public function getCmsStaticPage()
    {
        return $this->cmsStaticPage;
    }

    /**
     * Set createdAt
     *
     * @param \DateTime $createdAt
     *
     * @return CmsStaticPageImage
     */
    public function setCreatedAt($createdAt)
    {
        $this->createdAt = $createdAt;

        return $this;
    }

    /**
     * Get createdAt
     *
     * @return \DateTime
     */
    public function getCreatedAt()
    {
        return $this->createdAt;
    }

    /**
     * Set extension
     *
     * @param string $extension
     *
     * @return CmsStaticPageImage
     */
    public function setExtension($extension)
    {
        $this->extension = $extension;

        return $this;
    }

    /**
     * Get extension
     *
     * @return string
     */
    public function getExtension()
    {
        return $this->extension;
    }
}
