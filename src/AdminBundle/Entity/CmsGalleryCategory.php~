<?php

namespace AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CmsGalleryCategory
 *
 * @ORM\Table(name="cms_gallery_category")
 * @ORM\Entity(repositoryClass="AdminBundle\Repository\CmsGalleryCategoryRepository")
 */
class CmsGalleryCategory
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }
}
