{# @var cmsStaticPage \AdminBundle\Entity\CmsStaticPage #}
{# @var image \AdminBundle\Entity\CmsStaticPageFile #}
{% if images is not empty %}
    <div class="gallery-preview-images">
        {% for image in images %}
            <a href="{{ asset('thumb.php') }}?src={{ asset(cmsStaticPage.filesDir ~ image.fileName) }}&w=1024">
                <img src="{{ asset('thumb.php') }}?src={{ asset(cmsStaticPage.filesDir ~ image.fileName) }}&w=60&h=60"
                     class="img-thumbnail lazy-load"
                     data-original="{{ asset('thumb.php') }}?src={{ asset(cmsStaticPage.filesDir ~ image.fileName) }}&w=60&h=60"
                >
            </a>
        {% endfor %}
    </div>
{% else %}
    ---
{% endif %}